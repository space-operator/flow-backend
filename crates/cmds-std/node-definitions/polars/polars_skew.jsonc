{
  "$schema": "https://schema.spaceoperator.com/node-v2.schema.json",
  "version": "0.1",
  "name": "polars_skew",
  "prefix": "polars",
  "type": "native",
  "description": "Compute column-wise skewness of numeric columns in a DataFrame. Positive skew indicates a right tail, negative skew indicates a left tail.",
  "author_handle": "spo",
  "source_code": "private-cmds/src/polars/polars_skew.rs",
  "ports": {
    "inputs": [
      {
        "name": "dataframe",
        "type_bounds": [
          "string"
        ],
        "required": true,
        "tooltip": "DataFrame in IPC format"
      },
      {
        "name": "columns",
        "type_bounds": [
          "json"
        ],
        "tooltip": "Array of column names to aggregate (omit for all numeric columns)"
      },
      {
        "name": "bias",
        "type_bounds": [
          "bool"
        ],
        "tooltip": "If false, applies correction for statistical bias (sample skewness)"
      }
    ],
    "outputs": [
      {
        "name": "dataframe",
        "type": "string",
        "tooltip": "Aggregated DataFrame in IPC format (1-row result)"
      },
      {
        "name": "dataframe_json",
        "type": "json",
        "optional": true,
        "tooltip": "Aggregated DataFrame as JSON (for viewing)"
      }
    ]
  },
  "config_schema": {},
  "config": {
    "bias": {
      "B": true
    }
  },
  "classification": {
    "vendor": "Native",
    "program": "polars",
    "category": "data",
    "icon_url": null,
    "tags": [
      "data",
