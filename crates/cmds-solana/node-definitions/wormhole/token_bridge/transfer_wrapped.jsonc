{
  "$schema": "https://schema.spaceoperator.com/node-v2.schema.json",
  "version": "0.1",
  "name": "transfer_wrapped",
  "prefix": "wormhole",
  "description": "Transfer wrapped tokens from Solana to another chain via the Wormhole token bridge",
  "type": "native",
  "author_handle": "spo",
