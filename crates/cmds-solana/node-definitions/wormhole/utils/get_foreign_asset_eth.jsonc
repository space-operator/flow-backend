      {
        "name": "chain_id",
        "type_bounds": [
          "number"
        ],
        "required": true,
        "passthrough": false,
        "tooltip": "Wormhole chain ID of the token's origin chain"
      },
      {
        "name": "network",
        "type_bounds": [
          "string"
        ],
        "required": true,
        "passthrough": false,
        "tooltip": "Ethereum network name to query"
      }
    ],
    "outputs": [
      {
        "name": "address",
        "type": "string",
        "tooltip": "Wrapped token address on Ethereum"
      }
    ]
