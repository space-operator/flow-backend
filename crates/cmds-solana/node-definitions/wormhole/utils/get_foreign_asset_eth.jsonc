      {
        "name": "token",
        "type_bounds": [
          "string"
        ],
        "required": true,
        "passthrough": false,
        "tooltip": "Token address on the origin chain"
      },
      {
        "name": "is_nft",
        "type_bounds": [
          "bool"
        ],
        "required": false,
        "passthrough": false,
        "tooltip": "Whether the asset is an NFT (default: false)"
